        {
            "app": "camera",
            "label": "Camera",
            "icon_path": "${LAUNCHER_APP_ROOT}/icons/app_buttons/camera_64x64.png",
            "exe_path": {
                "win32": "explorer.exe"
            },
            "exe_args": [
                "shell:appsFolder\\Microsoft.WindowsCamera_8wekyb3d8bbwe!App"
            ]
