    "user_info_cache": {
        "_type_": "single_path_create",
        "win32": [
            "${HOME}/.pxlc_PyWebBrowserApp",
            "${USERPROFILE}/AppData/Local/pxlc_PyWebBrowserApp"
        ],
        "default": [ "${HOME}/.pxlc_PyWebBrowserApp" ]
    },
    "user_temp_root": {
        "_type_": "single_path_create",
        "win32": [
            "${TEMP}/pxlc_PyWebBrowserApp_temp_${USERNAME}",
            "${USERPROFILE}/AppData/Local/Temp/pxlc_PyWebBrowserApp_temp"
        ],
        "default": [ "/usr/tmp/pxlc_PyWebBrowserApp_temp_${USER}" ]
    },

    "port_cycle_range": {"start": 40000, "end": 42000},

    "pythonpath_additions": {
        "_type_": "multi_path",
        "win32": [
        ],
        "default": []
    }
}
